package day01Prac.정렬;

import java.util.Arrays;

public class BookMain {

	public static void main(String[] args) {
		
		
		// 책정보배열
		
		Book[] books = new Book[5];
		books[0] = new Book("재미있는책",100);
		books[1] = new Book("무서운책",50);
		books[2] = new Book("신나는",200);
		books[3] = new Book("웃긴책",180);
		books[4] = new Book("행복한 책",30);

		
		//
		// 정렬
		
//		Arrays.sort(books);
		MyArrays.sort(books);
		// sort라는 라이브러리를 사용하려면 정렬의 기준 MyComparable을 구현해야 한다
	
		
		// 정렬된 후
		for( Book b : points) {
			System.out.println(b);
		}
	}

}

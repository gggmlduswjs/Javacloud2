package day01Prac.정렬;


// 객체를 만들려면 클래스가 필요하다
// 객체(데이터(속성) +기능(매서드))


// Acorn 클래스이다
// 객체를 만들기 위한 설계도이다
// 객체는 new Acorn() 에 의해서 만들어짐!!
public class Acorn implements Comparable{
	
	// 데이터
	private String id;
	private String name;
	private int point;
	
	public Acorn() {
		
		// TODO Auto-generated constructor stub
	}
	
	
	
	// 매서드
	
	// 값넣기
	public Acorn(String id, String name, int point) {
//		super();
		this.id = id;
		this.name = name;
		this.point = point;
	}
	// 값꺼내기
	@Override
	public String toString() {
		return "Acorn [id=" + this.id + ", name=" + name + ", point=" + point + "]";
	}
}

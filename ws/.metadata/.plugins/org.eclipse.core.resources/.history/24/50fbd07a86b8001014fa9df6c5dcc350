package day01Prac.래퍼클래스;

import java.util.ArrayList;

public class Ex01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a=10;	//기본형 자료형(int, double, char, boolean) , 기본형변수가 만들어짐
		
		// int => Integer
		
		Integer num =a; // 오토박싱
		num = Integer.valueOf(a); 	// 명시적인 박싱
		
		
		int b = num.intValue();	//명시적인 언박싱	
		
		System.out.println(b);
		
		b=num; // 오토 언박싱
		
		System.out.println(b);
		
		
		ArrayList list = new ArrayList();
		list.add(3);	// 내부적으로 오토박싱 int -> Integer
		list.add(Integer.valueOf(3)); // 명시적인 박싱
		
		char c='A';
		
		Character cc=c;	// 오토박싱
		cc=Character.valueOf(c);	// 명시적인 박싱

	}

}

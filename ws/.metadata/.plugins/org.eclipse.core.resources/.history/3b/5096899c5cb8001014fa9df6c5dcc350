package day01Prac.라이브러리만들기;

public class MyLibMain {
	
	public static void main(String[] args) {
		MyLib lib = new MyLib();
		lib.printStarThree();
		
		lib.printChar('-');
		lib.printChar('김');
		
		//
		
		// 1. 인터페이스를 구현한 클래스 작성하기
		// 2. 객체 생성하기
		
		// 이름있는 클래스 만들기
		lib.runCode(null);
		
	}
}

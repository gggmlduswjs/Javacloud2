package day01Prac.정렬;

public class Book implements MyComparable{
	
	
	
	public Book() {
		super();
		// TODO Auto-generated constructor stub
	}
	String name;
	int point;
	public Book(String name, int point) {
		super();
		this.name = name;
		this.point = point;
	}
	@Override
	public String toString() {
		return "Book [name=" + name + ", point=" + point + "]";
	}
	@Override
	public int compareTo(Object other) {
		// TODO Auto-generated method stub
		Book book=(Book)other;
		
		return this.point = book.point;	// 기준이 크면 양수가 무조건 전달됨
	}
	
	//
	

}
